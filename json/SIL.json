[
    {
        "code": "SIL_5/SIL118",
        "author": "Armand Silvestre",
        "work": "LE PAYS DES ROSES",
        "date": "1880-1882",
        "title": "LE PAYS DES ROSES",
        "profile": "8",
        "structure": "suite p\u00e9riodique",
        "url": "https://crisco2.unicaen.fr/verlaine/index.php?navigation=textesauteurs&auteur=SIL_5&code_text=SIL118_7",
        "text": [
            [
                {
                    "verse": "Je sais, du profond de mon \u00eatre,",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Un coin plein d'immortelles fleurs,",
                    "metre": "8",
                    "rhyme": "b"
                },
                {
                    "verse": "Oasis o\u00f9 nul ne p\u00e9n\u00e8tre",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Que le soleil et l'aube en pleurs.",
                    "metre": "8",
                    "rhyme": "b"
                }
            ],
            [
                {
                    "verse": "Je sais un jardin plein de roses",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Au c\u0153ur comme le mien ouvert,",
                    "metre": "8",
                    "rhyme": "b"
                },
                {
                    "verse": "Et qui, dans les hivers moroses,",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Gardent un printemps toujours vert.",
                    "metre": "8",
                    "rhyme": "b"
                }
            ],
            [
                {
                    "verse": "Elles sont rouges, et leur s\u00e8ve",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Est comme un sang au mien pareil ;",
                    "metre": "8",
                    "rhyme": "b"
                },
                {
                    "verse": "Leur parfum flotte avec mon r\u00eave",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Dessus leur calice vermeil.",
                    "metre": "8",
                    "rhyme": "b"
                }
            ],
            [
                {
                    "verse": "S\u0153urs vivantes de mes pens\u00e9es,",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Je les retrouve en moi toujours,",
                    "metre": "8",
                    "rhyme": "b"
                },
                {
                    "verse": "Toujours tremblantes et bless\u00e9e",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Au souffle amer de mes amours.",
                    "metre": "8",
                    "rhyme": "b"
                }
            ],
            [
                {
                    "verse": "Alentour de leur tige fr\u00eale",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Mes chansons viennent se poser",
                    "metre": "8",
                    "rhyme": "b"
                },
                {
                    "verse": "Et les font vibrer sous leur aile",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Avec un bruit lent de baisers.",
                    "metre": "8",
                    "rhyme": "b"
                }
            ],
            [
                {
                    "verse": "Que leur douceur me soit ravie,",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "Et je meurs d\u00e9sesp\u00e9r\u00e9ment.",
                    "metre": "8",
                    "rhyme": "b"
                },
                {
                    "verse": "Seule, leur ombre sur ma vie",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "A pench\u00e9 quelque enchantement.",
                    "metre": "8",
                    "rhyme": "b"
                }
            ],
            [
                {
                    "verse": "Mon c\u0153ur saigne quand on les cueille,",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "\u2014 Saisi d'un caprice pervers,",
                    "metre": "8",
                    "rhyme": "b"
                },
                {
                    "verse": "C'est pour toi que je les effeuille",
                    "metre": "8",
                    "rhyme": "a"
                },
                {
                    "verse": "O toi pour qui j'\u00e9cris ces vers !",
                    "metre": "8",
                    "rhyme": "b"
                }
            ]
        ]
    }
]